{
  "id": null,
  "uid": "mlops-overview",
  "title": "MLOps Pipeline Overview",
  "tags": [
    "mlops",
    "crypto",
    "predictor"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Pipeline Status",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "up{job=\"model-api\"}",
          "legendFormat": "Model API"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                }
              }
            },
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(predictions_total)",
          "legendFormat": "Total"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "Prediction Latency (p95)",
      "type": "gauge",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(prediction_latency_seconds_bucket[5m]))",
          "legendFormat": "p95"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "max": 1,
          "min": 0,
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.1
              },
              {
                "color": "red",
                "value": 0.5
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Feedback Received",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(feedback_total)",
          "legendFormat": "Feedback"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "purple",
                "value": null
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Predictions Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "rate(predictions_total[5m])",
          "legendFormat": "{{status}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "gradientMode": "scheme"
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 6,
      "title": "Latency Distribution",
      "type": "heatmap",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "rate(prediction_latency_seconds_bucket[5m])",
          "format": "heatmap"
        }
      ]
    },
    {
      "id": 7,
      "title": "Database - Raw Data Count",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as count FROM warehouse.raw_market_data",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Raw Records"
        }
      }
    },
    {
      "id": 8,
      "title": "Latest Price (BTC/USD)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "rawSql": "SELECT price FROM warehouse.raw_market_data ORDER BY ingested_at DESC LIMIT 1",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "displayName": "BTC Price"
        }
      }
    },
    {
      "id": 9,
      "title": "Data Ingestion Rate",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 12
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as count FROM warehouse.raw_market_data WHERE ingested_at > NOW() - INTERVAL '1 hour'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Records/Hour"
        }
      }
    },
    {
      "id": 10,
      "title": "Price History",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "rawSql": "SELECT ingested_at as time, price FROM warehouse.raw_market_data ORDER BY ingested_at DESC LIMIT 100",
          "format": "time_series"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "currencyUSD",
          "displayName": "BTC Price"
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}